{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "gcc-7 build active file",
			"command": "/usr/bin/gcc-7",
			"args": [
				"-g",
				"-o",
				"${fileDirname}/p5",
				"${fileDirname}/*.c",
			],
			"options": {
				"cwd": "/usr/bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}